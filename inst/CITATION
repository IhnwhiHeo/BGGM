year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)


bibentry(bibtype = "Article",
         title = "BGGM: Bayesian Gaussian Graphical Models in R",
         author = c(person("Donald", "Williams"),
                    person("Joris", "Mulder")),
         year = "2019",
         journal = "PsyArXiv",
         note = note,
         url = "https://psyarxiv.com/t2cn7/")
